{
	"compilerOptions": {
		"module": "ES6",
		"moduleResolution": "Bundler",
		"baseUrl": ".",
		"paths": {
			"@src/pages/*" : ["src/pages/*"],
			"@src/assets/*" : ["src/assets/*"],
			"@src/components/*" : ["src/components/*"],
            "@src/layouts/*" : ["src/layouts/*"],
            "@src/supabase/*" : ["src/supabase/*"],
            "@src/custom-hooks/*" : ["src/custom-hooks/*"],
            "@src/redux/*" : ["src/redux/*"],
		},
		"jsx": "react-jsx"
	}
}